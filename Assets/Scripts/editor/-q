#region Assembly UnityEngine.AnimationModule, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null
// UnityEngine.AnimationModule.dll
#endregion


namespace UnityEngine
{
    //
    // Summary:
    //     Used to communicate between scripting and the controller. Some parameters can
    //     be set in scripting and used by the controller, while other parameters are based
    //     on Custom Curves in Animation Clips and can be sampled using the scripting API.
    [NativeAsStructAttribute]
    [NativeHeaderAttribute("Modules/Animation/AnimatorControllerParameter.h")]
    [NativeHeaderAttribute("Modules/Animation/ScriptBindings/AnimatorControllerParameter.bindings.h")]
    [NativeTypeAttribute(Bindings.CodegenOptions.Custom, "MonoAnimatorControllerParameter")]
    [UsedByNativeCodeAttribute]
    public class AnimatorControllerParameter
    {
        public AnimatorControllerParameter();

        //
        // Summary:
        //     The name of the parameter.
        public string name { get; set; }
        //
        // Summary:
        //     Returns the hash of the parameter based on its name.
        public int nameHash { get; }
        //
        // Summary:
        //     The type of the parameter.
        public AnimatorControllerParameterType type { get; set; }
        //
        // Summary:
        //     The default float value for the parameter.
        public float defaultFloat { get; set; }
        //
        // Summary:
        //     The default int value for the parameter.
        public int defaultInt { get; set; }
        //
        // Summary:
        //     The default bool value for the parameter.
        public bool defaultBool { get; set; }

        public override bool Equals(object o);
        public override int GetHashCode();
    }
}